<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="view.EView"
            id="DiagramaFlojo.view1"
            name="Diagrama"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="Diagrama"
               mnemonic="M">
            <command
                  commandId="DiagramaFlojo.command4"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
  
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="diagramaflojo.Ejecutar"
            commandId="DiagramaFlojo.command4">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="DiagramaFlojo.category4"
            id="DiagramaFlojo.command4"
            name="Ejecutar">
      </command>
      <category
            id="DiagramaFlojo.category4"
            name="Categoria">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="DiagramaFlojo.command4"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+6">
      </key>
   </extension> 

</plugin>
